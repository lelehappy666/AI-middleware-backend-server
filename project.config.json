{
  "name": "AI中台后端服务器项目",
  "version": "1.0.0",
  "description": "基于React + TypeScript + Vite + Node.js + Prisma的全栈项目",
  "urls": {
    "frontend": "http://localhost:5173",
    "backend": "http://localhost:3000",
    "api": "http://localhost:3001"
  },
  "ports": {
    "frontend": 5173,
    "backend": 3000,
    "api": 3001
  },
  "database": {
    "type": "SQLite",
    "orm": "Prisma",
    "file": "./prisma/dev.db"
  },
  "tech_stack": {
    "frontend": ["React", "TypeScript", "Vite", "TailwindCSS", "Zustand"],
    "backend": ["Node.js", "Express", "TypeScript", "Prisma"],
    "database": ["SQLite", "Prisma ORM"]
  },
  "scripts": {
    "dev": "启动开发服务器 (前端: 5173, 后端: 3000)",
    "build": "构建生产版本",
    "check": "代码检查和构建"
  },
  "notes": {
    "frontend_port": "前端页面地址固定为 http://localhost:5173，不可修改",
    "backend_port": "后端API服务地址为 http://localhost:3000",
    "proxy_target": "前端代理目标为 http://localhost:3001"
  }
}